{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
         "no-trailing-spaces": "error",
        // "eol-last": ["error", "always"],
        // "eqeqeq": ["error", "always"],
        "max-len": ["error", { "code": 180 }]
    }
}
